  $tableArr = array(
    'consultant firm' => 'tblconsultantfirms',
    'office' => 'tblconsultantoffices',
    'manager firm' => 'tblmanagerfirms',
    'plan' => 'tblfunds',
    'plan-history' => 'tblfunds',
